{
  "name": "wpjsapi-lib",
  "version": "3.1.1",
  "description": "Library to use WordPress REST API easily in any JS project",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayach90/wpjs-api.git"
  },
  "keywords": [
    "WordPress",
    "JS",
    "API",
    "REST",
    "API"
  ],
  "author": "Ã€ngel Ayach Boadas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ayach90/wpjsapi/issues"
  },
  "homepage": "https://github.com/Ayach90/wpjsapi#readme",
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@vitest/coverage-v8": "^2.1.9",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^2.1.9"
  }
}
